{
    "datasets basepath": "./datasets",
    "use all datasets": false,
    "load one dataset at a time": false,
    "datasets folders": [
        "adult census income",
        "contraceptive",
        "contraceptive with aggreg",
        "cover type",
        "cover type with aggreg",
        "kdd98 multiclass 2",
        "kdd98 multiclass 9",
        "san francisco crime 15 classes",
        "shelter animal outcomes 15 classes"
    ],

    "criteria": [
        "Twoing",
        "LS Squared Gini",
        "LS Chi Square",
        "PC-ext",
        "Hypercube Cover"
    ],

    "use numeric attributes": true,

    "output folder": "./outputs/d1/w_num",

    "num trials": 20,
    "calculate t-test on accuracy": true,
    "t-test single-sided p-value": 0.05,

    "max depth": 1,
    "rank attributes": false,
    "use cross-validation": true,
    "num folds": 3,
    "is stratified": true,

    "prunning parameters": {
        "use chi-sq test": true,
        "max chi-sq p-value": 0.1,
        "second most freq value min samples": 15,

        "min num samples allowed": [1],
        "use second most freq class min samples": false
    }
}
