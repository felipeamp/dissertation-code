{
    "datasets basepath": "./datasets",
    "use all datasets": false,
    "load one dataset at a time": false,
    "datasets folders": [
        "reuters phonemes 10000 samples 15 classes",
        "san francisco crime 15 classes"
    ],

    "criteria": [
        "Conditional Inference Tree Twoing",
        "Conditional Inference Tree LS Squared Gini",
        "Conditional Inference Tree LS Chi Square",
        "Conditional Inference Tree PC-ext"
